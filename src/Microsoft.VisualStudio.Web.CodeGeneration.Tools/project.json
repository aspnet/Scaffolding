{
  "version": "1.0.0-preview4-update1",
  "buildOptions": {
    "keyFile": "../../tools/Key.snk",
    "outputName": "dotnet-aspnet-codegenerator",
    "emitEntryPoint": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "description": "Code Generation tool for ASP.NET Core. Contains the dotnet-aspnet-codegenerator command used for generating controllers and views. ",
  "configurations": {
    "debug_x86": {
      "buildOptions": {
        "platform": "anycpu32bitpreferred"
      }
    },
    "release_x86": {
      "buildOptions": {
        "platform": "anycpu32bitpreferred"
      }
    }
  },
  "dependencies": {
    "Microsoft.VisualStudio.Web.CodeGeneration": "1.0.0-preview4-update1",
    "Microsoft.Extensions.DotnetToolDispatcher.Sources": {
      "version": "1.0.1",
      "type": "build"
    }
  },
  "frameworks": {
    "net451": {},
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.4",
          "type": "platform"
        }
      }
    }
  },
  "packOptions": {
    "tags": [
      "aspnetcore",
      "codegenerator",
      "scaffolding",
      "visualstudioweb"
    ]
  }
}