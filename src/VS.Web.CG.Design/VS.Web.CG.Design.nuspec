<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <authors>$authors$</authors>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">$license$</license>
    <description>$description$</description>
    <icon>packageIcon.png</icon>
    <projectUrl>https://github.com/aspnet/scaffolding</projectUrl>
    <copyright>$copyright$</copyright>
    <tags>$tags$</tags>
    <repository type="git" url="$repositoryUrl$" commit="$repositoryCommit$" />
    <dependencies>
      <group targetFramework=".NETFramework4.6.1">
        <dependency id="Microsoft.VisualStudio.Web.CodeGenerators.Mvc" version="$version$" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".netcoreapp3.0">
        <dependency id="Microsoft.VisualStudio.Web.CodeGenerators.Mvc" version="$version$" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>

  <files>
    <file src="$packageIconFullPath$" target="" />
    <file src="$OutputBinary$" target="lib\netcoreapp3.0\$AssemblyName$.dll" />
    <file src="$OutputDocumentation$" target="lib\netcoreapp3.0\$AssemblyName$.xml" />
    <file src="$OutputSymbol$" target="lib\netcoreapp3.0\$AssemblyName$.pdb" />

    <file src="$OutputAnyExe$" target="lib\net461\$AssemblyName$.exe" />
    <file src="$OutputAnyDocumentation$" target="lib\net461\$AssemblyName$.xml" />
    <file src="$OutputAnySymbol$" target="lib\net461\$AssemblyName$.pdb" />

    <file src="$OutputX64Exe$" target="runtimes\win7-x64\lib\net461\$AssemblyName$.exe" />
    <file src="$OutputX64Documentation$" target="runtimes\win7-x64\lib\net461\$AssemblyName$.xml" />
    <file src="$OutputX64Symbol$" target="runtimes\win7-x64\lib\net461\$AssemblyName$.pdb" />

    <file src="$OutputX86Exe$" target="runtimes\win7-x86\lib\net461\$AssemblyName$.exe" />
    <file src="$OutputX86Documentation$" target="runtimes\win7-x86\lib\net461\$AssemblyName$.xml" />
    <file src="$OutputX86Symbol$" target="runtimes\win7-x86\lib\net461\$AssemblyName$.pdb" />

    <file src="$OutputARMExe$" target="runtimes\win-arm\lib\net461\$AssemblyName$.exe" />
    <file src="$OutputARMDocumentation$" target="runtimes\win-arm\lib\net461\$AssemblyName$.xml" />
    <file src="$OutputARMSymbol$" target="runtimes\win-arm\lib\net461\$AssemblyName$.pdb" />

    <file src="$OutputARM64Exe$" target="runtimes\win-arm64\lib\net461\$AssemblyName$.exe" />
    <file src="$OutputARM64Documentation$" target="runtimes\win-arm64\lib\net461\$AssemblyName$.xml" />
    <file src="$OutputARM64Symbol$" target="runtimes\win-arm64\lib\net461\$AssemblyName$.pdb" />
  </files>
</package>